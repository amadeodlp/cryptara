# Production Environment Variables
JWT__KEY=your_production_jwt_key
JWT__ISSUER=COSMOSBackend
JWT__AUDIENCE=COSMOSFrontend
JWT__EXPIRYINMINUTES=60

# Production Blockchain Configuration 
BLOCKCHAIN__INFURAURL=https://mainnet.infura.io/v3/your_production_infura_id
BLOCKCHAIN__TOKENCONTRACTADDRESS=your_production_token_address
BLOCKCHAIN__WALLETCONTRACTADDRESS=your_production_wallet_address
BLOCKCHAIN__STAKINGCONTRACTADDRESS=your_production_staking_address

# Production Database (SQL Server)
CONNECTIONSTRINGS__DEFAULTCONNECTION=Server=your_production_server;Database=Finance;User Id=your_production_username;Password=your_production_password;TrustServerCertificate=True;
ASPNETCORE_ENVIRONMENT=Production